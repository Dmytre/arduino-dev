void setup() {
  Serial.begin(9600);

  // Инициализация дисплея
  display.begin();

  // Очищаем дисплей
  display.clearDisplay();
  display.display();

  // Устанавливаем контраст
  display.setContrast(24);
  delay(1000);
}


void loop() {
  // Рисуем заранее подготовленное лого
  // Подготовлен массив из 16 пар байтов
  // каждый байт состоит из 8 битов, соответсвенно
  // получаем матрицу 16х16 битов, 1-черный цвет, 0-белый цвет
  display.drawBitmap(0, 0, logoBmp1, 85, 44, BLACK); // x, y, logo, w, h, color 
  display.display();
  delay(200);
  display.clearDisplay();
  display.drawBitmap(0, 0, logoBmp2, 85, 44, BLACK); // x, y, logo, w, h, color 
  display.display();
  delay(200);
  display.clearDisplay();
}
